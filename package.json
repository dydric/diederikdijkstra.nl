{
  "name": "diederikdijkstra",
  "version": "3.0.1",
  "description": "Personal website with 11ty, TailwindsCSS and PostCSS.",
  "scripts": {
    "start": "concurrently \"ELEVENTY_ENV=development eleventy --serve --watch\" \"postcss src/_assets/stylesheets/app.pcss -o src/static/app.compiled.css -w\" \"webpack --watch --config webpack.config.js\"",
    "minify": "del dist && ELEVENTY_ENV=production eleventy && purgecss --config ./purgecss.config.js --out src/_includes/stylesheets && ELEVENTY_ENV=production webpack --config webpack.config.js",
    "build": "npm run minify && ELEVENTY_ENV=online eleventy --serve",
    "online": "npm run minify && ELEVENTY_ENV=online eleventy"
  },
  "author": "Diederik Dijkstra",
  "license": "ISC",
  "devDependencies": {
    "@11ty/eleventy": "^0",
    "@babel/core": "^7",
    "@babel/preset-env": "^7",
    "autoprefixer": "^9",
    "babel-loader": "^8",
    "concurrently": "^5",
    "del-cli": "^3",
    "dotenv-webpack": "^1",
    "html-minifier": "^4",
    "luxon": "^1",
    "node-instagram": "^4",
    "postcss-cli": "^6",
    "postcss-import": "^12.0.1",
    "postcss-nested": "4.2.1",
    "postcss-preset-env": "^6.5.0",
    "purgecss": "^1",
    "spotify-web-api-node": "^4",
    "tailwindcss": "^1",
    "tumblr": "^0",
    "twitter": "^1",
    "webpack": "^4",
    "webpack-cli": "^3"
  }
}
